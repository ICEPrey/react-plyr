{
  "name": "webpack-react-typescript-starter-pack",
  "version": "1.0.0",
  "description": "webpack react typescript starter pack",
  "main": "./src/index.tsx",
  "private": true,
  "repository": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/**/* --ext .js, .jsx, .ts, .tsx; exit 0",
    "lint:fix": "npm run lint --fix",
    "test:e2e": "node_modules/.bin/nightwatch -c ./tests/e2e/nightwatch.conf.js --env chrome",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch",
    "start:dev": "NODE_ENV=development webpack-dev-server --config ./config/development/webpack.config.js --mode development",
    "start:prod": "NODE_ENV=production node server --colors --progress --watch",
    "build:dev": "NODE_ENV=development webpack --mode development --config ./config/development/webpack.config.js --progress --watch",
    "build:staging": "NODE_ENV=staging webpack --mode development --config ./config/staging/webpack.config.js --progress",
    "build:prod": "NODE_ENV=production webpack --mode production --config ./config/production/webpack.config.js --progress",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "autoprefixer": "^9.7.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-import": "^2.19.1",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.19.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "8.0.0",
