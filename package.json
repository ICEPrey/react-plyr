    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .; exit 0",
    "start": "NODE_ENV=development node scripts/start.js",
    "build": "rm -rf build && NODE_ENV=production node scripts/build.js",
    "test": "node scripts/test.js",
    "storybook": "start-storybook -s ./public -p 6008",
    "storybook-build": "rm -rf docs && build-storybook -c .storybook -o docs && copyfiles -u 1 public/* docs",
    "publish:npm": "rm -rf ./build && NODE_ENV=production webpack --mode production --config ./config/publish/webpack.config.js --progress"
