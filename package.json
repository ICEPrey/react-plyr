    "tslint-react": "^4.1.0",
    "typescript": "^3.7.4",
    "resolve": "^1.14.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1",
    "webpack-dotenv-plugin": "^2.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "dependencies": {
    "plyr": "^3.5.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
