  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .; exit 0",
    "test:e2e": "node_modules/.bin/nightwatch -c ./tests/e2e/nightwatch.conf.js --env chrome",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook-build": "rimraf docs && build-storybook -c .storybook -o docs && copyfiles -u 1 public/* docs",
    "start:dev": "NODE_ENV=development webpack-dev-server --config ./config/development/webpack.config.js --mode development",
    "start:prod": "NODE_ENV=production webpack-dev-server --config ./config/development/webpack.config.js --mode production",
    "build:dev": "NODE_ENV=development webpack --mode development --config ./config/development/webpack.config.js --progress --watch",
    "build:staging": "NODE_ENV=staging webpack --mode development --config ./config/staging/webpack.config.js --progress",
    "build:prod": "NODE_ENV=production webpack --mode production --config ./config/production/webpack.config.js --progress",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
